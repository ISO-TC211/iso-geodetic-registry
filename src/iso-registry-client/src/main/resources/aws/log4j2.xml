<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%-5p: %c - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="${LOG_LEVEL:-ERROR}">
			<AppenderRef ref="console" />
		</Root>
	</Loggers>
</Configuration>
