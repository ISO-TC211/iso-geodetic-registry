<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment />
	<entry key="hibernate.connection.username">${hibernate.connection.username}</entry>
	<entry key="hibernate.connection.password">${hibernate.connection.password}</entry>
	<entry key="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</entry>
	<entry key="hibernate.connection.url">${hibernate.connection.url}</entry>
	<entry key="hibernate.connection.driver_class">org.postgresql.Driver</entry>
</properties>
